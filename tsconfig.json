{
    "compilerOptions": {
        "target": "es2015",
        "outDir": "lib",
        "declarationDir": "lib/typings",
        "sourceMap": false,
        "declaration": true,
        "module": "es2015",
        "moduleResolution": "node",
        "removeComments": false,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"noImplicitUseStrict": true,
		"noImplicitAny": true,
		"lib": ["es5", "es2015.iterable", "dom"]
    },
    "exclude": [
        "node_modules",
        "lib"
    ]
}